package iHateEvenSubarrays;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class _3IHateEvenSubarrays {

	static String getString() throws IOException {
		InputStreamReader isr = new InputStreamReader(System.in);
		BufferedReader br = new BufferedReader(isr);
		String s = br.readLine();
		return s;
	}
	
	public static void main(String[] args) throws IOException {
		int T = Integer.parseInt(getString());
		String minimalStrings[] = new String[T];
		int prevStrLen = 0, currStrLen = 0;
		String str;
		
		for (int t=0; t<T; t++) {
			str = getString();
			prevStrLen = str.length();
			currStrLen = 0;

			while (prevStrLen > currStrLen) {
				prevStrLen = str.length();
				str = str.replace("00", "");
				str = str.replace("11", "");
				System.out.println(prevStrLen + " @ " + " @ "+ currStrLen);
				currStrLen = str.length();
			}
			
			if (str.isEmpty())
				minimalStrings[t] = "EMPTY";
			else
				minimalStrings[t] = str;
		}
		
		for (int t=0; t<T; t++) {
			System.out.println(minimalStrings[t]);
		}
	}
}